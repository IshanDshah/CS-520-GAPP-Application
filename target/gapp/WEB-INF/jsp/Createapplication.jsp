<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
	pageEncoding="ISO-8859-1"%>

<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>

<%@ taglib prefix="form" uri="http://www.springframework.org/tags/form"%>

<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">


<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<link rel="stylesheet"
	href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
<script
	src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
<script
	src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>



<title>Create Application</title>

</head>
<body>

	<div id="header">
		<h4>
			<b>Personal Information</b>
		</h4>
	</div>
	<form:form modelAttribute="student" method="post"
		action="Createapplication.html" class="form" style="width: 25%">
		<div class="form-group">
			First Name <input type="text" placeholder="First Name"
				name="firstName" class="form-control .col-sm-*" required />

		</div>
		<div class="form-group">
			Last Name <input type="text" placeholder="Last Name" name="lastName"
				class="form-control .col-sm-*" required />
		</div>
		<div class="form-group">
			Email <input type="email" placeholder="Email" name="email"
				class="form-control .col-sm-*" required />
		</div>
		<div class="form-group">
			CIN <input type="text" placeholder="CIN" name="CIN"
				class="form-control .col-sm-*" required />
		</div>
		<div class="form-group">
			Phone Number <input type="text" placeholder="Phone Number"
				name="PhoneNO" style="width: auto" class="form-control .col-sm-*"
				required />
		</div>

		<div class="form-group">
			Gender <input type="text" placeholder="Gender" name="Gender"
				class="form-control .col-sm-*" required />
		</div>

		<div class="form-group">
			D.O.B <input type="text" placeholder="Date of Birth" name="DOB"
				class="form-control .col-sm-*" required />
		</div>

		<div class="form-group">
			Citizenship <input type="text" placeholder="CitizenShip"
				name="citizen" class="form-control .col-sm-*" required />
		</div>

		<div class="form-group">
			Toefl <input type="text" placeholder="Toefl"
				class="form-control .col-sm-*" name="Toefl">
		</div>

		<div class="form-group">
			GRE <input type="text" placeholder="GRE"
				class="form-control .col-sm-*" name="Gre">
		</div>
		<div class="form-group">
			GPA <input type="text" label="GPA" class="form-control .col-sm-*"
				name="Gpa">
		</div>

		<input type="hidden" value="${applicationid}" name="applicationid">
		<input type="submit" value="Next" class="btn btn-default">
	</form:form>







</body>
</html>
